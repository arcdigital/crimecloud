{
	"manifest_version": 2,
	"name": "CrimeCloud",
	"version": "1.1",
	"description": "Replaces the text 'Cloud' with 'Crime'.",
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
